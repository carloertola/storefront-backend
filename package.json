{
  "name": "storefront-backend",
  "version": "1.0.0",
  "description": "database and RESTful API for storefront-backend",
  "main": "server.ts",
  "scripts": {
    "prettier": "prettier --config .prettierrc.json \"src/**/*.ts\" --write",
    "lint": "eslint . --ext .ts",
    "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./build/server.js\"",
    "test": "ENV='test' && npx tsc && db-migrate --env test up && jasmine && db-migrate db:drop test",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carloertola/storefront-backend.git"
  },
  "keywords": [
    "storefront-backend",
    "udacity",
    "database",
    "RESTful",
    "API"
  ],
  "author": "Carlo Ertola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carloertola/storefront-backend/issues"
  },
  "homepage": "https://github.com/carloertola/storefront-backend#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^8.4.10",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.16",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  }
